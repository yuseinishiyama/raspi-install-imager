network:
  version: 2
  ethernets:
    eth0:
      dhcp4: false
      addresses:
      {{- range $i, $address := .Addresses }}
      - {{ $address }}
      {{- end }}
      gateway4: 192.168.2.1
      nameservers:
        addresses:
        {{- range $i, $address := .Nameservers.Addresses }}
        - {{ $address }}
        {{- end }}
        search:
        {{- range $i, $search := .Nameservers.Search }}
        - {{ $search }}
        {{- end }}
